{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/marchev",
  "description": "Randomized Clockify timesheets generator",
  "default_locale": "en",
  "permissions": [
    "https://api.clockify.me/",
    "https://global.api.clockify.me/",
    "https://cdn.statically.io/",
    "notifications"
  ],
  "content_security_policy": "script-src 'self' https://cdn.panelbear.com; object-src 'self'",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "__MSG_extName__",
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  }
}
